        <item name="android:navigationBarDividerColor">?themeNavigationDivider</item>
    </style>

    <style name="Base.Theme.LightOne.V27" parent="Base.Theme.Light.V27">
        <item name="themeNavigationBar">@color/colorNavigationBar</item>
        <item name="themeNavigationDivider">@color/colorNavigationDivider</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>

    <style name="Base.Theme.LightTwo.V27" parent="Base.Theme.Light.V27">
