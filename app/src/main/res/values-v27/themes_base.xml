    <style name="Base.AppTheme.LightTwo" parent="Base.Theme.LightTwo.V27"/>

    <style name="Base.AppTheme.Dark.V27" parent="Base.AppTheme.Dark.V23">
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@null</item>
    </style>

    <style name="Base.AppTheme.Dark" parent="Base.AppTheme.Dark.V27" />

