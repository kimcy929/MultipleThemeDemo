<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Base.Theme.DayNight.V27" parent="Base.Theme.DayNight.V23">
        <item name="android:windowLightNavigationBar">@bool/light_status_bar</item>
        <item name="android:navigationBarColor">?themeNavigationBar</item>
        <item name="android:navigationBarDividerColor">?themeNavigationDivider</item>
    </style>

    <style name="Base.AppTheme.DayNight" parent="Base.Theme.DayNight.V27">
        <item name="themeNavigationBar">@color/colorNavigationBar</item>
        <item name="themeNavigationDivider">@color/colorNavigationDivider</item>
    </style>

    <style name="Base.Theme.Light.V27" parent="Base.Theme.Light.V23">
